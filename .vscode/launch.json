        {
            "type": "node",
            "request": "launch",
            "name": "Launch Prepare",
            "runtimeVersion": "8.1.2",
            "program": "${workspaceFolder}/prepare.js",
            "args": [
                "{tree:'IIFE'}"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch BUILD",
            "runtimeVersion": "8.1.2",
            "program": "${workspaceFolder}/build.js",
